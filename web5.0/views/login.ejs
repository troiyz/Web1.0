<html>

<head>
  <title>Firebase Login</title>
  <link href="https://fonts.googleapis.com/css?family=Nunito:400,600,700" rel="stylesheet">
  <link type="text/css" rel="stylesheet" href="https://cdn.firebase.com/libs/firebaseui/3.0.0/firebaseui.css" />
  <link rel="stylesheet" href="Style/css/style2.css" />
  <%- include scripts.ejs %>
</head>

<body>


  <div id="login_div" class="main-div">
    <h3>Firebase Web login</h3>
    <input type="email" placeholder="Email..." id="email_field" name="Email" required/>
    <input type="password" placeholder="Password..." id="password_field" name="Password" required/>
    <button id="myButton" onclick="login()">Login to Account</button>
  </div>

  <div id="user_div" class="loggedin-div">
    <h3>Welcome</h3>
    <p id="user_para">Welcome to Firebase web login. You're currently logged in.</p>
  </div>

  <script src="Style/js/login.js"></script>
  <script>
    $("#password_field").keyup(function (event) {
      if (event.keyCode === 13) {
        $("#myButton").click();
      }
    });

    $("#myButton").click(function () {
      
    });
    </script>

</body>

</html>